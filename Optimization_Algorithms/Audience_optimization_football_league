    {
      "cell_type": "code",
      "source": [
        "#FUNCIÓN PARA CALCULAR LA AUDIENCIA AJUSTADA PARA UNA DETERMINADA COMBINACIÓN DE PARTIDOS Y HORARIOS\n",
        "def calcular_audiencia_ajustada(partidos,horarios):\n",
        "  audiencia = 0\n",
        "  for i in range(0,len(partidos)):   #Iteramos sobre la longitud de las listas de partidos y horarios (ambas tienen la misma longitud)\n",
        "    audiencia_base = audiencias_base[partidos[i]]\n",
        "    penalizacion_horarios = penalizaciones_horarios[horarios[i]]\n",
        "\n",
        "    #Calculamos cuantas veces está ese horario en lista. Las coincidencias se obtienen restando 1\n",
        "    coincidencias = horarios.count(horarios[i]) - 1\n",
        "    penalizacion_coincidencias = penalizaciones_coincidencias[coincidencias]\n",
        "\n",
        "    audiencia_partido = audiencia_base * penalizacion_horarios * penalizacion_coincidencias\n",
        "    audiencia += audiencia_partido\n",
        "\n",
        "  return round(audiencia,3)\n",
        "\n",
        "\n",
        "#FUNCIÓN PARA MODIFICAR LOS HORARIOS ALEATORIAMENTE\n",
        "def modifica_horario(horarios):\n",
        "    nuevos_horarios = horarios.copy()\n",
        "\n",
        "    while True:\n",
        "        indices = random.sample(range(len(horarios)), 2)  # Cambiar 2 horarios por otros aleatorios\n",
        "        for i in indices:\n",
        "            nuevos_horarios[i] = random.choice(horarios_posibles)\n",
        "\n",
        "        if nuevos_horarios.count(\"V20\") == 1 and nuevos_horarios.count(\"L20\") == 1:   # Aseguramos que siempre haya exactamente un \"V20\" y un \"L20\"\n",
        "            return nuevos_horarios\n",
        "\n",
        "\n",
        "#FUNCIÓN PARA CALCULAR LA PROBABILIDAD DE ACEPTAR PEORES SOLUCIONES\n",
        "def probabilidad(T,d):\n",
        "  return random.random() <  math.exp( -1*d / T)"
      ],
      "metadata": {
        "id": "mRCXRP6fdaNr"
      },
      "execution_count": 11,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#ALGORITMO DE RECOCIDO SIMULADO A PARTIR DE UNA OPCIÓN FACTIBLE\n",
        "\n",
        "def recocido_simulado(Temperatura, partidos_ini, horarios_ini):\n",
        "  #Probamos a aplicar el recocido simulado de manera que los horarios iniciales y los partidos iniciales son elegidos al azar.\n",
        "\n",
        "  horarios_ref = horarios_ini.copy()\n",
        "  partidos = partidos_ini.copy()\n",
        "  audiencia_ref = calcular_audiencia_ajustada(partidos, horarios_ref)\n",
        "  print(\"La audiencia obtenida con la distribución de partidos inicial es de \", audiencia_ref, \" millones de espectadores\")\n",
        "\n",
        "  mejor_audiencia = audiencia_ref  #Audiencia a mejorar\n",
        "  mejor_distribucion_horarios = horarios_ref.copy()\n",
        "\n",
        "  iter=0\n",
        "\n",
        "  while Temperatura> 0.0001:\n",
        "    iter+=1\n",
        "    #Generamos horarios vecinos\n",
        "    nuevo_horario = modifica_horario(horarios_ref)\n",
        "    nueva_audiencia = calcular_audiencia_ajustada(partidos, nuevo_horario)\n",
        "\n",
        "    #Calculamos la diferencia de audiencia y si es la mejor la almacenamos\n",
        "    if nueva_audiencia > mejor_audiencia:\n",
        "      mejor_audiencia = nueva_audiencia\n",
        "      mejor_distribucion_horarios = nuevo_horario\n",
        "\n",
        "    #Si la nueva es mejor se cambia.\n",
        "    #Si es peor la referencia se cambia según la probabilidad que depende de T y d\n",
        "    if nueva_audiencia > audiencia_ref or probabilidad(Temperatura,abs(audiencia_ref - nueva_audiencia)):\n",
        "      horarios_ref = nuevo_horario\n",
        "      audiencia_ref = nueva_audiencia\n",
        "\n",
        "    #Bajamos la temperatura gradualmente:\n",
        "    Temperatura = Temperatura * 0.999\n",
        "\n",
        "  print(\"A continuación se muestra la mejor distribución de horarios para esta jornada:\\n\")\n",
        "  for i in range(len(partidos)):\n",
        "    print(partidos[i], \"-->\", mejor_distribucion_horarios[i])\n",
        "  print(\"\\nGenerando una audiencia de\", mejor_audiencia, \" millones de espectadores\")\n",
        "\n",
        "  #return partidos, mejor_distribucion_horarios\n",
        "  #Se puede descomentar el return si necesitamos almacenar las variables para futuros calculos. Si solo queremos saber la distribución de horarios, nos sirve con el print (más visual)\n"
      ],
      "metadata": {
        "collapsed": true,
        "id": "37Jof_99nyhm"
      },
      "execution_count": 12,
      "outputs": []
    },
